{
  "version": "1.0",
  "testCases": [
    {
      "documentation": "Access an array index at index 0",
      "params": {
        "ResourceList": ["resource"]
      },
      "expect": {
        "endpoint": {
          "url": "https://www.resource.example.com"
        }
      }
    },
    {
      "documentation": "Fallback when array is unset",
      "params": {
        "ResourceList": []
      },
      "expect": {
        "endpoint": {
          "url": "https://www.example.com"
        }
      }
    }
  ]
}
